{
  "name": "nrafunded",
  "version": "2.0.2",
  "description": "Let's take Congress back from the NRA.",
  "private": true,
  "scripts": {
    "build": "nwb build-react-app",
    "clean": "nwb clean-app",
    "start": "nwb serve-react-app --port 4000",
    "scrape": "node parse/scraper.js",
    "parse": "node parse/parse.js",
    "data": "npm run scrape && npm run parse",
    "render": "npm run render:analysis && npm run render:body",
    "render:analysis": "static-react --no-doctype src/Analysis.js > public/analysis.html",
    "render:body": "static-react --no-doctype src/Body.js > public/body.html"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "comma-number": "^2.0.0",
    "html-loader": "^0.5.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.0",
    "nwb": "0.14.x",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-geomicons": "^2.1.0",
    "rebass": "^1.0.4",
    "scrape-it": "^3.3.3",
    "static-react": "^3.2.0",
    "unformat-usd": "^0.1.1",
    "velocity-react": "^1.3.3"
  },
  "author": "Lachlan Campbell <lachlan.campbell@icloud.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lachlanjc/nrafunded"
  }
}
